    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@rollup/plugin-terser": "^0.4.3",
    "commitizen": "^4.3.0",
    "commitlint": "^17.6.3",
    "commitlint-config-cz": "^0.13.3",
    "commitlint-config-gitmoji": "^2.3.1",
    "cz-conventional-changelog": "^3.3.0",
