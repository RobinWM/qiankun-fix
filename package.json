{
  "name": "qiankun-fix",
  "description": "An JavaScript fixer for qiankun sub-apps",
  "private": true,
  "scripts": {
    "build:main": "rollup -c -w",
    "preinstall": "node ./script/preinstall.js",
    "eslint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "commit": "cz"
  },
  "author": "RobinWM",
  "license": "ISC",
  "type": "commonjs",
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@babel/preset-env": "^7.21.5",
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@rollup/plugin-terser": "^0.4.3",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.40.0",
    "husky": "^8.0.3",
    "prettier": "^2.8.8",
    "rollup": "^3.22.0"
  },
  "workspaces": [
    "antd/*",
    "elementUI/*"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
